{
  "name": "@cookshack/bred",
  "version": "0.1.2",
  "description": "browser editor thing",
  "main": "js/main.mjs",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "start-gdb": "gdb -q --args node node_modules/.bin/electron .",
    "start": "electron .",
    "inspect": "electron --inspect=5656 .",
    "mime": "./bin/generate-mime-by-ext",
    "check": "./node_modules/@cookshack/build/bin/check js/*.mjs js/*.js ext/*/*.mjs",
    "format": "./node_modules/@cookshack/build/bin/format js/*.mjs js/*.js ext/*/*.mjs",
    "prepare": "husky",
    "postinstall": "make prep && sha256sum package-lock.json | cut -d' ' -f1 > .bred-module-hash"
  },
  "author": "Matt Mundell",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^40.0.0",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "jshint": "^2.13.6",
    "mocha": "^10.7.3",
    "peggy": "^4.0.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "@codemirror/state": "$@codemirror/state",
    "@codemirror/view": "$@codemirror/view",
    "@codemirror/language": "$@codemirror/language",
    "@codemirror/commands": "$@codemirror/commands",
    "@codemirror/search": "$@codemirror/search",
    "@codemirror/lint": "$@codemirror/lint",
    "@lezer/common": "1.5.0",
